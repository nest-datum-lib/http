{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "moduleDirectories": [
    "node_modules",
    "src",
    "@nest-datum/*",
    "@nest-datum-lib/*",
    "@nest-datum-common/*",
    "@nest-datum-utils/*"
  ],
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/$1",
    "^@nest-datum/(.*)$": "<rootDir>/@nest-datum/$1",
    "^@nest-datum-common/(.*)$": "<rootDir>/@nest-datum-common/$1",
    "^@nest-datum-lib/(.*)$": "<rootDir>/@nest-datum-lib/$1",
    "^@nest-datum-utils/(.*)$": "<rootDir>/@nest-datum-utils/$1"
  },
  "rootDir": "../../",
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "**/*.(t|j)s"
  ],
  "coverageDirectory": "../coverage",
  "testEnvironment": "node"
}